CPU   = 68020
FLAGS = NMINC STRMERGE CPU=$(CPU) IDIR=//Include
OBJS = NiKomFido.o

all: NiKomFido

clean:
  -delete \#?.o
  -delete NiKomFido

NiKomFido: $(OBJS)
  slink FROM lib:c.o $(OBJS) \
        TO NiKomFido \
        LIB lib:sc.lib+lib:amiga.lib \
        NOALVS NOICONS NODEBUG

NiKomFido.o: NiKomFido.c
  sc $(FLAGS) NiKomFido.c
